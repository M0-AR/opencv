# OpenCV Toolkit Environment Variables

# Development Settings
DEBUG=True
LOG_LEVEL=DEBUG

# Processing Settings
MAX_IMAGE_SIZE=1920x1080
JPEG_QUALITY=95
VIDEO_FRAME_RATE=30

# Feature Detection
MIN_MATCH_COUNT=10
FEATURE_DETECTION_ALGORITHM=SIFT
